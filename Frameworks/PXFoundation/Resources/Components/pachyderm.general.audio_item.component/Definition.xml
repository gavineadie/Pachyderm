<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<component-definition>
	<identification>
		<localized-name>
			<string lang="en">Audio Item</string>
		</localized-name>
		<version>100</version>
		<version-string>Version 1.0, Pachyderm 3.0, 2014-Dec-19</version-string>
		<component-identifier>pachyderm.general.audio_item</component-identifier>
		<conforming-identifiers>
			<identifier>pachyderm.abstract.item</identifier>
		</conforming-identifiers>
	</identification>
	<binding-list>
		<binding type="attribute" container="none">
			<binding-key>audio</binding-key>
			<localized-name>
				<string lang="en">Audio File</string>
			</localized-name>
			<content-types>
				<identifier>public.audiovisual-content</identifier>
				<identifier>pachyderm.resource</identifier>
			</content-types>
			<help-tag></help-tag>
			<x-help-text>Choose an audio file.</x-help-text>
			<editable>YES</editable>
			<default-association><association><!-- insert serialized association here --></association></default-association>
			<limits>
			</limits>
		</binding>
		<binding type="attribute" container="none">
			<binding-key>caption</binding-key>
			<localized-name>
				<string lang="en">Caption</string>
			</localized-name>
			<content-types>
				<identifier>public.text</identifier>
			</content-types>
			<help-tag></help-tag>
			<x-help-text>Enter a caption.</x-help-text>
			<editable>YES</editable>
			<default-association><association><!-- insert serialized association here --></association></default-association>
				<limit for="public.text" strict="YES">
					<key>max-length</key>
					<integer>60</integer>
				</limit>
		</binding>
	</binding-list>
	<binding-validators>
	</binding-validators>
	<user-interface>
		<preferred-binding-ordering>
			<binding-key>audio</binding-key>
			<binding-key>caption</binding-key>
		</preferred-binding-ordering>
	</user-interface>
	<environment-support>
		<environment>
			<identifier>pachyderm.flash-environment</identifier>
			<min-size>{0.0, 0.0}</min-size>
			<max-size>{0.0, 0.0}</max-size>
			<auto-layout>NO</auto-layout>
			<accepts-children>NO</accepts-children>
			<resources>
				<resource></resource>
			</resources>
		</environment>
	</environment-support>
</component-definition>