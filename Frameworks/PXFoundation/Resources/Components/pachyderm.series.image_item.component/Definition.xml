<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<component-definition>
	<identification>
		<localized-name>
			<string lang="en">Series Page</string>
		</localized-name>
		<version>100</version>
		<version-string>Version 1.0, Pachyderm 3.0, 2014-Dec-19</version-string>
		<component-identifier>pachyderm.series.image_item</component-identifier>
		<conforming-identifiers>
			<identifier>pachyderm.abstract.item</identifier>
		</conforming-identifiers>
	</identification>
	<binding-list>
		<binding type="attribute" container="none">
			<binding-key>thumbnail</binding-key>
			<localized-name>
				<string lang="en">Series Page Thumbnail</string>
			</localized-name>
			<content-types>
				<identifier>public.image</identifier>
				<identifier>pachyderm.resource</identifier>
			</content-types>
			<help-tag></help-tag>
			<x-help-text>Enter your series page thumbnail.</x-help-text>
			<editable>YES</editable>
			<default-association><association><!-- insert serialized association here --></association></default-association>
			<limits>
				<limit for="public.image" strict="YES">
					<key>min-size</key>
					<size>{128, 96}</size>
					<key>max-size</key>
					<size>{128, 96}</size>
				</limit>
			</limits>
		</binding>
		<binding type="attribute" container="none">
			<binding-key>image</binding-key>
			<localized-name>
				<string lang="en">Series Image Item</string>
			</localized-name>
			<content-types>
				<identifier>public.image</identifier>
				<identifier>pachyderm.resource</identifier>
			</content-types>
			<help-tag></help-tag>
			<x-help-text>Choose an image item for the page.</x-help-text>
			<editable>YES</editable>
			<default-association><association><!-- insert serialized association here --></association></default-association>
			<limits>
				<limit for="public.image" strict="YES">
					<key>min-size</key>
					<size>{1, 1}</size>
					<key>max-size</key>
					<size>{2560, 2048}</size>
				</limit>
			</limits>
		</binding>
	</binding-list>
	<binding-validators>
	</binding-validators>
	<user-interface>
		<preferred-binding-ordering>
			<binding-key>thumbnail</binding-key>
			<binding-key>image</binding-key>
		</preferred-binding-ordering>
	</user-interface>
	<environment-support>
		<environment>
			<identifier>pachyderm.flash-environment</identifier>
			<min-size>{0.0, 0.0}</min-size>
			<max-size>{0.0, 0.0}</max-size>
			<auto-layout>NO</auto-layout>
			<accepts-children>NO</accepts-children>
			<resources>
				<resource></resource>
			</resources>
		</environment>
	</environment-support>
</component-definition>
